// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  token String
  id Int @id
  email String 
  login String
  first_name String
  last_name String
  large_pic String
  medium_pic String
  small_pic String
  createAt DateTime @default(now())
  updateAt DateTime @updatedAt
}

model Data {
  code String @unique
}